# AWS RDS Database Configuration (Dashboard)
DB_HOST=your-database-host.rds.amazonaws.com
DB_USER=your_db_user
DB_PASSWORD=your_db_password
DB_NAME=your_database_name
DB_PORT=3306

# Chatbot Database Configuration
CHATBOT_DB_HOST=your-chatbot-database-host.rds.amazonaws.com
CHATBOT_DB_USER=your_chatbot_user
CHATBOT_DB_PASSWORD=your_chatbot_password
CHATBOT_DB_DATABASE=your_chatbot_database
CHATBOT_DB_PORT=3306

# API Keys
OPENAI_API_KEY=your_openai_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here

# Application Configuration
API_PORT=8000
API_HOST=0.0.0.0
API_PORT_CHATBOT=8002
API_HOST_CHATBOT=0.0.0.0

# CORS Configuration
ALLOWED_ORIGINS=*
ALLOW_CREDENTIALS=false

# Environment
ENV=production
